<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd
                           http://www.springframework.org/schema/context
                           http://www.springframework.org/schema/context/spring-context.xsd">
    <bean name="book1" class="com.springcore.AutoWire.ViaXml.Book1">
        <property name="bookId" value="110" />
        <property name="bookName" value="Harry Potter"/>
        <property name="price" value="1499"/>
    </bean>
    <bean name="book2" class="com.springcore.AutoWire.ViaXml.Book2">
        <property name="bookId" value="114" />
        <property name="bookName" value="Harry Potter"/>
        <property name="price" value="2899"/>
    </bean>
    <bean name="library" class="com.springcore.AutoWire.ViaXml.Library" autowire="byName"/>
</beans>